<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c" %>

<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>종합예제</title>
</head>
<body>
    <h3>set, out</h3>
    <c:set var="product1" value="<b>애플 이어폰</b>" />
    <c:set var="product2" value="삼성 갤럭시 노트" />
     <c:set var="intArray" value="${1,2,3,4,5}" />

    <p>
    product[jstl]:
    <c:out value="${product1}" default="Not register" escapeXml="true" />
    </p>
    <p>product1[e]:<c:out value="${product1}" /></p>
   
</body>
</html>
